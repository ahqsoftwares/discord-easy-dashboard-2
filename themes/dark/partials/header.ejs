<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-dark-5@1.1.3/dist/css/bootstrap-night.min.css" rel="stylesheet">
        
    <%- dashboardConfig.injectCSS ? `<style>${dashboardConfig.injectCSS}</style>` : '' %>
</head>

<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
      <a class="navbar-brand"><%= bot?.user?.username %></a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarText">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <% if (page == "home") {%>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/">Home</a>
          </li>
          <% } else { %>
            <li class="nav-item">
              <a class="nav-link" aria-current="page" href="/">Home</a>
            </li>
            <% } %>
            <% if(hasClientSecret) { %>
              <% if (page == "commands") {%>
                <li class="nav-item">
                  <a class="nav-link active" href="/commands">Commands</a>
                </li>
                <% } else { %>
                  <li class="nav-item">
                    <a class="nav-link" href="/commands">Commands</a>
                  </li>
                <% } %>
            <% } %>
          <% if(hasClientSecret) { %>
            <% if (page == "dashboard") {%>
              <li class="nav-item">
                <a class="nav-link active" href="/selector">Dashboard</a>
              </li>
              <% } else { %>
                <li class="nav-item">
                  <a class="nav-link" href="/selector">Dashboard</a>
                </li>
              <% } %>
          <% } %>
          
          <li class="nav-item">
            <a class="nav-link" href="<%= dashboardDetails.inviteUrl ? dashboardDetails.inviteUrl : `https://discord.com/oauth2/authorize?client_id=${bot.user.id}&scope=bot&permissions=8` %>">Invite</a>
          </li>
          <% if(dashboardDetails.serverUrl) { %>
            <li class="nav-item">
              <a class="nav-link" href="<%= dashboardDetails.serverUrl %>">Discord server</a>
            </li>
          <% } %>
          <li class="nav-item"><a href="/home" class="nav-link">Light Mode</a></li>
        </ul>
        <% if(hasClientSecret) { %>
          <ul class="navbar-nav">
            <% if (user) { %>
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarScrollingDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false"><img src="https://cdn.discordapp.com/avatars/<%=user.id%>/<%=user.avatar%>.png", style="width: 30px;margin-bottom:3%;"alt="LOGO">
                  Logged in as <%= user.username %>
                </a>
                <ul class="dropdown-menu" aria-labelledby="navbarScrollingDropdown">
                  <!-- <li><hr class="dropdown-divider"></li> -->
                  <li><a class="dropdown-item" href="/auth/logout">Logout</a></li>
                </ul>
              </li>
              </li>
            <% } else { %>
              <li class="nav-item"><a href="/auth/login" class="nav-link">Login</a></li>
            <% } %>
          </ul>
        <% } %>
      </div>
    </div>
  </nav>